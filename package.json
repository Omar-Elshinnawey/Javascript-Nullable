{
  "name": "optionull",
  "version": "0.3.0",
  "description": "An implementation for the nullable design pattern in javascript",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "nyc mocha",
    "build": "babel ./src --source-maps-inline -d ./dist",
    "browserify": "browserify ./dist/optionull.js --standalone Optional -o ./dist/optionull.js",
    "uglify": "uglifyjs --compress --mangle -o ./dist/optionull.min.js -- ./dist/optionull.js"
  },
  "keywords": [
    "Nullable",
    "Optional"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Omar-Elshinnawey/optionull.git"
  },
  "author": "omardev <elshinnaweyom@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "chai": "^4.1.2",
    "grunt": "^1.0.2",
    "grunt-browserify": "^5.3.0",
    "grunt-contrib-clean": "^1.1.0",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "uglifyify": "^4.0.5"
  }
}
